@model IEnumerable<Library.UI.Models.Category>
@{
    Layout = null;
}
@Html.ActionLink("Home", "AllBooksList", "Books", null, new { @class = "side-nav-button" })
@foreach (var link in Model)
{
    @Html.RouteLink(link.Name, new {controller = "NavigationMenu", action = "Menu", category = link.Id, page = 1}, new {@class = "side-nav-button"})
}